            <string>
//Interacting
canInteract = collision_circle(x,y,25,obj_interactable,false,true);
var interacting = canInteract &amp;&amp; keyboard_check(vk_space);

if(interacting) {
    canInteract.bar += 15/60;
    canInteract.alarm[0] = 1*60;
    canInteract.pauseBar = true;
}

//Speed Control
if(speed &gt; maxSpd) {
    speed = maxSpd;
}

if(speed &lt; 0) {
    speed = 0;
}

if(interacting &amp;&amp; speed &gt; 0) {
    speed = maxSpd/8
}

//Keyboard controls
if((keyboard_check(ord("W")) || keyboard_check(ord("A")) || keyboard_check(ord("S")) || keyboard_check(ord("D")))) {
    speed += spd;
}

if(keyboard_check(ord("S"))) {
    direction = 270;
}
if(keyboard_check(ord("W"))) {
    direction = 90;
}
if(keyboard_check(ord("D"))) {
    direction = 0;
}
if(keyboard_check(ord("A"))) {
    direction = 180;
}

if(keyboard_check(ord("S")) &amp;&amp; keyboard_check(ord("A"))) {
    direction = 225;
}
if(keyboard_check(ord("S")) &amp;&amp; keyboard_check(ord("D"))) {
    direction = 315;
}
if(keyboard_check(ord("W")) &amp;&amp; keyboard_check(ord("A"))) {
    direction = 135;
}
if(keyboard_check(ord("W")) &amp;&amp; keyboard_check(ord("D"))) {
    direction = 45;
}


</string>
